<% if @features.empty? %>
	<p>
<%=   t('found.none.records', :what => t('associate.d.record', :what => Feature.model_name.human)).capitalize.s %>
	</p>
<% else %>
	<ul style="list-style-type:square; margin-left:2em;">
	<% for feature in @features do %>
	  <li class="normal"><%= link_to h(feature.prioritized_name(current_view)), feature_path(feature.fid) %></li>
	<% end %>
	</ul>
	<p class="pagination" style="text-align: left;"><%= will_paginate @features, :container => false %></p>
<% end %>