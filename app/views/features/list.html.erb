<%= render :partial => "features/top" %>  
<div class="left" id="RelatedPlaces">
  <div class="has-right-margin has-ajax-pagination">
    <h2>Places Associated with <%= @object_title %></h2>
    <div class="pagination-click-results">
<%=   render :partial => "features/list" %>
    </div>
    <p>
<%=   link_to ts(:in, :what => @object_title, :where => @object_type.humanize.pluralize), @object_url %>
    </p>
  </div>
</div>
<%= javascript_on_load "document.title = 'THL Place Dictionary of Tibet & the Himalayas'" %>
<script type="text/javascript">
jQuery(document).ready(function(){
  // Ajaxify the pagination for lists of features
  jQuery('.has-ajax-pagination .pagination a').live('click', function() {
    var this_link = this;
    jQuery.ajax({
      type: 'GET',
      url: this.href,
      success: function(html){
        jQuery(this_link).parents('.pagination-click-results:first').html(html);
      }
    });
    return false;
  });
});
</script>
<%= render :partial => "features/bottom" %>