<%  header_options = { :class => 'places', :javascript_files => ['kmaps_engine/admin', 'subjects_integration/application'], :stylesheet_files => ['kmaps_engine/admin'] }
    header_options[:title] = "#{model_name.classify.constantize.model_name.human.titleize}: #{controller.action_name.humanize}" if defined?(model_name) && !model_name.blank? %>
<%= header(header_options) %>
<div class="shell-1">		
<% if @page_title %>
     <div id="header">
       <h2><%= @page_title %></h2>
     </div>
<% end %>			
   <div id="bcNav" class="left">
     <div class="left"><%= render_breadcrumbs %></div>
     <div class="right"><%= resource_nav %></div>
   </div>			
   <br class="clear"/>			
   <div id="content-main">
<%=  yield %>
   </div>		
</div>
<%= footer %>