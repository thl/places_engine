<% add_breadcrumb_item link_to('citation', polymorphic_path([:admin, @parent_object.citable, @parent_object]))
add_breadcrumb_item 'pages'
add_breadcrumb_item link_to(@object.id, object_path(@object))
add_breadcrumb_item 'edit'
form_for :page, @object, :url=>object_path, :html=>{:method=>:put} do |f| %>
<%= render :partial=>'form_fields', :locals=>{:f=>f,:object=>@object} %>
<%= link_to 'Cancel', collection_path %> | <%= submit_tag 'Update' %>
<% end %>