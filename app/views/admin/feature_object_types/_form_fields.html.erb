<%= error_messages_for model_name %>
<%=	category_fields({
		:subject => {:display => f_label(@object.feature), :label => f.label(:feature)},
		:root => @parent_object_type,
		:var_name => :feature_object_type,
		:include_js => false,
		:include_styles => true,
		:selectable => true,
		:single_selection => single_selection,
		:extra_fields => [
			{:field => f.text_field(:string_value, :style => 'padding:3px; width: 300px'), :label => f.label(:text_value)},
			{:field => f.text_field(:numeric_value, :style => 'padding:3px; width: 300px'), :label => f.label(:numeric_value)},
			{:field => f.check_box(:show_parent), :label => "Show category\'s immediate parent"},
			{:field => f.check_box(:show_root), :label => 'Show top level Knowledge Map'},
			{:field => "#{f.text_field(:label)} #{f.radio_button(:prefix_label, true)} Prefix with > &nbsp;&nbsp;&nbsp;&nbsp;#{f.radio_button(:prefix_label, false)} 'SPACE suffix'", :label => CategoryFeature.human_attribute_name(:label)}]
