<% add_breadcrumb_item features_link
   add_breadcrumb_item feature_link(parent_object) if parent_object
   add_breadcrumb_item FeatureObjectType.model_name.human(:count => :many).s %>
<div>
  <div class="right highlight">
<%  if parent? %>
<%=   new_item_link [parent_object, :feature_object_type] %>
<%  else %>
<%=   ts 'creat.e.requirement', :what => FeatureRelation.model_name.human, :whom => Feature.model_name.human %>
<%  end %>
  </div>
</div>
<br class="clear"/>
<section class="panel panel-content">
  <div class="panel-heading">
    <h6><%= FeatureObjectType.model_name.human.titleize.s %></h6>
  </div>
  <div class="panel-body">
<%= render :partial => 'list', :locals => { :list => @collection } %>
  </div> <!-- END panel-body -->
</section> <!-- END panel -->
