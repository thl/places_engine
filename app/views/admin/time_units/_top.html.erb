<% @object = @time_unit %>
<%= render :partial => 'admin/default/breadcrumbs' %>
<% add_breadcrumb_item link_to('dates', collection_path)
case params[:action]
when 'edit'
	add_breadcrumb_item link_to(@object.to_s, object_path(@object))
	add_breadcrumb_item 'edit'
when 'new'
	add_breadcrumb_item 'new'
when 'show'
	add_breadcrumb_item @object.to_s
when 'index' %>
<div>
	<div class="left">
	<%= resource_search_form %>
	</div>
	<div class="right highlight">
	<% if parent? %>
		<%= new_item_link [@parent_object, :time_unit] %>
	<% else %>
		<%= parent_resource_dependency_message %>
	<% end %>
	</div>
</div>

<br class="clear"/>	
<% end %>