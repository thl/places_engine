<% @object = @time_unit %>
<%= render :partial => 'admin/default/breadcrumbs' %>
<% add_breadcrumb_item link_to(ts('date.this', :count => :many), collection_path)
   case params[:action]
   when 'edit'
     add_breadcrumb_item link_to(@object.to_s, object_path(@object))
     add_breadcrumb_item ts('edit.this')
   when 'new'
     add_breadcrumb_item ts('new.this')
   when 'show'
     add_breadcrumb_item @object.to_s
   when 'index' %>
   <div>
     <div class="left">
<%=  resource_search_form %>
     </div>
     <div class="right highlight">
<%   if parent? %>
<%=    new_item_link [@parent_object, :time_unit] %>
<%   else %>
<%=    parent_resource_dependency_message %>
  <% end %>
     </div>
   </div>
   <br class="clear"/>  
<% end %>