<%= render :partial => "breadcrumbs" %>
<% add_breadcrumb_item link_to(@object.to_s, object_path(@object))
   add_breadcrumb_item ts('edit.this')
   form_for :association_note, @object, :url=>object_path, :html=>{:method=>:put} do |f| %>
<%=  render :partial=>'form_fields', :locals=>{:f=>f,:object=>@object} %>
<%=  link_to ts('cancel.this'), collection_path %> | <%= globalized_submit_tag 'update.this' %>
<% end %>