<% add_breadcrumb_item features_link
   add_breadcrumb_item feature_link(@object.feature)
   add_breadcrumb_item link_to(collection_name, collection_path)
   add_breadcrumb_item ts('new.this')
   form_for [:admin, @object] do |f| %>
<%=  hidden_field_tag 'feature_id', @object.feature.id %>
<%=  render :partial=>'form_fields', :locals=>{:f=>f,:object=>@object} %>
<%=  link_to ts('cancel.this'), collection_path %> | <%= globalized_submit_tag 'creat.e.record' %>
<% end %>