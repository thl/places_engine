<%  add_breadcrumb_items features_link
    add_breadcrumb_items feature_link(@parent_object)
    add_breadcrumb_items feature_relations_link(@parent_object)
    add_breadcrumb_items feature_relation_role_label(@parent_object, @object, :use_first=>false)
    add_breadcrumb_items ts('edit.this') %>
<%= form_for :feature_relation, @object, :url=>admin_feature_feature_relation_path(@parent_object, @object), :html=>{:method=>:put} do |f| %>
<%=   render :partial=>'form_fields', :locals=>{:f=>f} %>
<%=   link_to ts('cancel.this'), admin_feature_path(@parent_object) %> | <%= globalized_submit_tag 'update.this' %>
<%  end %>