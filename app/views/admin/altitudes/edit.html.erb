<%  add_breadcrumb_item features_link
    add_breadcrumb_item feature_link(parent_object)
    add_breadcrumb_item link_to(Altitude.model_name.human(count: :many).s, admin_feature_altitudes_path(parent_object))
    add_breadcrumb_item link_to(object.id, object_path(object))
    add_breadcrumb_item ts('edit.this') %>
<%= form_for object, url: object_path, method: :put do |f| %>
<%=   render partial: 'form_fields', locals: { f: f, object: object} %>
<%=   link_to ts('cancel.this'), collection_path, class: 'btn btn-primary form-submit', id: 'edit-cancel' %> | <%= globalized_submit_tag 'update.this', class: 'btn btn-primary form-submit' %>
<%  end %>