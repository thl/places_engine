<% add_breadcrumb_item features_link
   add_breadcrumb_item feature_link(@parent_object)
   add_breadcrumb_item link_to('altitudes', admin_feature_altitudes_path(@parent_object))
   add_breadcrumb_item 'new'
   form_for [:admin, @parent_object, @object] do |f| %>
<%=  f.hidden_field :feature_id %>
<%=  render :partial=>'form_fields', :locals=>{:f=>f,:object=>@object} %>
<%=  link_to 'Cancel', collection_path %> | <%= submit_tag 'Create' %>
<% end %>