<% options ||= {}
   if list.empty? %>
<%=  empty_collection_message("No altitudes available.") %>
<% else %>
<table class="listGrid">
<%= pagination_row :colspan=>6 unless @collection.nil? %>
	<tr>
		<th class="listActionsCol"></th>
		<th>Average</th>
		<th>Minimum</th>
		<th>Maximum</th>
		<th>Unit</th>
		<th>Notes</th>
		<th>Add Note</th>
		<th>Dates</th>
		<th>Add Date</th>
	</tr>
<%  list.each do |item| %>
	<tr>
		<td class="listActionsCol">
		  <%= list_actions_for_item(item,
  		  {
  		    :delete_path => polymorphic_path([:admin, item.feature, item]),
  		    :edit_path => edit_polymorphic_path([:admin, item.feature, item]),
  		    :view_path => polymorphic_path([:admin, item.feature, item])
  		  }
		  ) %>
		</td>		
		<td><%= item.average %></td>
		<td><%= item.minimum %></td>
		<td><%= item.maximum %></td>
		<td><%= item.unit.title %></td>
		<td><%= note_link_list_for item %></td>
		<td><%= new_note_link_for item %></td>
		<td><%= time_unit_link_list_for item %></td>
		<td><%= new_time_unit_link_for item %></td>
	</tr>
	<% end %>	
</table>
<% end %>