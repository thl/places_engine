    add_breadcrumb_item link_to(Shape.model_name.human(:count => :many).s, collection_path)
    add_breadcrumb_item ts('edit.this') %>
<%= form_for @object, :url => admin_feature_shape_path(@object.feature, @object), :method => :put do |f| %>
<%=   render :partial=>'form_fields', :locals=>{:f=>f,:object=>@object} %>
<%=   link_to ts('cancel.this'), collection_path %> | <%= globalized_submit_tag 'update.this' %>
<%  end %>