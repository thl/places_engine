<% add_breadcrumb_item feature_link(object.feature)
   add_breadcrumb_item link_to(Topic.human_name(:count => :many).s, admin_feature_category_features_path(object.feature))
   add_breadcrumb_item object.id %>
<section class="panel panel-content">
  <div class="panel-heading">
    <h6><%= Topic.model_name.human.titleize.s %></h6>
  </div>
  <div class="panel-body">
  <div id="accordion" class="panel-group">
<section class="panel panel-default">
    <div class="panel-heading">
      <h6><a href="#collapseOne" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle"><span class="glyphicon glyphicon-plus"></span><%= ts 'information.general' %></a></h6>
    </div>
    <div id="collapseOne" class="panel-collapse collapse in">
      <div class="panel-body">
<fieldset>
  <legend><%= "#{CategoryFeature.model_name.human.titleize.s} #{edit_item_link object}".html_safe %></legend>
  <div class="row">
    <label><%= Feature.model_name.human.titleize.s %></label>
    <span><%= def_if_blank(object, :feature, :name) %></span>
  </div>
  <div class="row">
    <label><%= Topic.human_name.titleize.s %></label>
    <span><%= object.stacked_category %></span>
  </div>
  <div class="row">
    <label><%= CategoryFeature.human_attribute_name(:string_value).s %></label>
    <span><%= def_if_blank object, :string_value %></span>
  </div>
  <div class="row">
    <label><%= CategoryFeature.human_attribute_name(:numeric_value).s %></label>
    <span><%= def_if_blank object, :numeric_value %></span>
  </div>
</fieldset>
    </div> <!-- END panel-body -->
  </div> <!-- END collapseOne -->
</section> <!-- END panel -->
<section class="panel panel-default">
    <div class="panel-heading">
      <h6><a href="#collapseTwo" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle collapsed"><span class="glyphicon glyphicon-plus"></span><%= ts 'Citations' %></a></h6>
    </div>
    <div id="collapseTwo" class="panel-collapse collapse">
      <div class="panel-body">
<%= citation_list_fieldset %>
    </div> <!-- END panel-body -->
  </div> <!-- END collapseTwo -->
</section> <!-- END panel -->
<section class="panel panel-default">
    <div class="panel-heading">
      <h6><a href="#collapseThree" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle collapsed"><span class="glyphicon glyphicon-plus"></span><%= ts 'Notes' %></a></h6>
    </div>
    <div id="collapseThree" class="panel-collapse collapse">
      <div class="panel-body">
<%= note_list_fieldset %>
    </div> <!-- END panel-body -->
  </div> <!-- END collapseThree -->
</section> <!-- END panel -->
<section class="panel panel-default">
    <div class="panel-heading">
      <h6><a href="#collapseFour" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle collapsed"><span class="glyphicon glyphicon-plus"></span><%= ts 'Dates' %></a></h6>
    </div>
    <div id="collapseFour" class="panel-collapse collapse">
      <div class="panel-body">
<%= time_unit_list_fieldset %>
    </div> <!-- END panel-body -->
  </div> <!-- END collapseFour -->
</section> <!-- END panel -->
  </div> <!-- END accordion -->
  </div> <!-- END panel-body -->
</section> <!-- END panel -->
