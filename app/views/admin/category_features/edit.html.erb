<%
add_breadcrumb_item features_link
add_breadcrumb_item feature_link(@object.feature)
add_breadcrumb_item link_to(collection_name, collection_path)
add_breadcrumb_item link_to(@object.id, object_path(@object))
add_breadcrumb_item 'edit'
%>

<% form_for [:admin, @object] do |f| %>

	<%= 
		category_form_table({
			:form_partial => 'form_fields',
			:footer => "#{link_to 'Cancel', object_path} | #{submit_tag 'Update'}",
			:locals => { :f => f, :object => @object }
		})
	%>

<% end %>