<% add_breadcrumb_item features_link
   add_breadcrumb_item feature_link(@object.feature)
   add_breadcrumb_item link_to(Topic.model_name.human(:count => :many).s, collection_path)
   add_breadcrumb_item ts('new.this') %>
<%= form_for [:admin, @object] do |f| %>
<%=   hidden_field_tag 'feature_id', @object.feature.id %>
<%=   category_form_table(:form_partial => 'form_fields',
        :footer => "#{link_to ts('cancel.this'), collection_path} | #{globalized_submit_tag 'creat.e.this'}",
        :locals => { :f => f, :object => @object, :single_selection => false }) %>
<%  end %>