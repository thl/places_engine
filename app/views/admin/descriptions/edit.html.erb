<%
add_breadcrumb_item features_link
add_breadcrumb_item feature_link(@object.feature)
add_breadcrumb_item feature_descriptions_link(@object.feature)
add_breadcrumb_item link_to(@object.id, admin_feature_description_path(parent_object,@object))
add_breadcrumb_item 'edit'
%>
<div>
<h1>Editing Description: <%= @object.id %></h1>
</div>
<br class="clear"/>

<% form_for @object, :url=>admin_feature_description_path(parent_object,@object), :html=>{:method=>:put} do |f| %>
	<%= render :partial=>'form_fields', :locals=>{:f=>f} %>
	<%= link_to 'Cancel', admin_feature_description_path(parent_object,@object) %> | <%= submit_tag 'Update' %>
<% end %>
