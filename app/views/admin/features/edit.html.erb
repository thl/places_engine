<% add_breadcrumb_items features_link, feature_link(@object), 'edit' %>

<% form_for :feature, @object, :url=>admin_feature_path(@object), :html=>{:method=>:put} do |f| %>
	<%= render :partial=>'form_fields', :locals=>{:f=>f} %>
	<%= link_to 'Cancel', admin_feature_path(@object) %> | <%= submit_tag 'Update' %>
<% end %>